<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<!-- edu.zju.cims201.GOF.hibernate.pojo.Author root -->
	<class name="edu.zju.cims201.GOF.hibernate.pojo.pdm.PdmProjectValuePool" table="PdmProjectValuePool">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="increment"/>
		</id>
	    <property name="value" column="value" type="string"/>
	    <property name="name" column="name" type="string"/>
	    <property name="iotype" column="iotype" type="int"/>
	    <property name="isarray" column="isarray" type="int"/>
		<many-to-one name="project" class="edu.zju.cims201.GOF.hibernate.pojo.pdm.PdmProject" column="pdmProjectId"></many-to-one>
	</class>
</hibernate-mapping>

	